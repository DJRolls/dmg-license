{
	"name": "dmg-license",
	"version": "0.2.1",
	"description": "Generate license agreements for macOS .dmg files",
	"author": "argvminusone",
	"license": "MIT",
	"repository": "argv-minus-one/dmg-license",
	"keywords": [
		"dmg",
		"licence",
		"license",
		"mac",
		"macos",
		"osx",
		"udif"
	],
	"bin": {
		"dmg-license": "./bin/dmg-license.js"
	},
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"clean": "rimraf lib",
		"prepare": "tslint --project . && tsc",
		"test": "mocha -r ts-node/register test/**.spec.ts",
		"regenerate-language-info": "node ./language-info-generator >language-info.json",
		"docs": "ts-node ./generate-supported-language-list.ts"
	},
	"files": [
		"bin/**/*.js",
		"language-info.json",
		"lib/**/*.js",
		"lib/**/*.d.ts",
		"schema.json"
	],
	"dependencies": {
		"ajv": "^6.10.0",
		"cli-truncate": "^1.1.0",
		"crc": "^3.8.0",
		"iconv-corefoundation": "^1.1.0",
		"plist": "^3.0.1",
		"smart-buffer": "^4.0.2",
		"verror": "^1.10.0"
	},
	"devDependencies": {
		"@types/chai": "^4.1.7",
		"@types/cli-truncate": "^1.1.0",
		"@types/crc": "^3.4.0",
		"@types/execa": "^0.9.0",
		"@types/minimist": "^1.2.0",
		"@types/mocha": "^5.2.6",
		"@types/node": "^11.12.0",
		"@types/plist": "^3.0.1",
		"@types/verror": "^1.10.3",
		"await-lock": "^1.1.3",
		"chai": "^4.2.0",
		"chai-bytes": "^0.1.2",
		"execa": "^1.0.0",
		"minimist": "^1.2.0",
		"mocha": "^6.1.4",
		"read-xml": "^3.0.0",
		"resourceforkjs": "^0.1.0",
		"rimraf": "^2.6.3",
		"ts-node": "^8.1.0",
		"tslint": "^5.15.0",
		"typescript": "^3.3.4000"
	},
	"engines": {
		"node": ">=5.10"
	},
	"os": [
		"darwin"
	]
}
